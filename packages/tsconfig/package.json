{
  "name": "@mvila/tsconfig",
  "version": "1.0.6",
  "description": "Shared TypeScript config used by mvila's projects",
  "keywords": [
    "tsconfig",
    "typescript",
    "ts",
    "config",
    "configuration"
  ],
  "author": "Manuel Vila <hi@mvila.me>",
  "license": "MIT",
  "repository": "https://github.com/mvila/util/tree/master/packages/tsconfig",
  "files": [
    "tsconfig.json"
  ],
  "main": "tsconfig.json",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "version:patch": "npm version --no-git-tag-version patch",
    "version:minor": "npm version --no-git-tag-version minor",
    "version:major": "npm version --no-git-tag-version major",
    "publish:package": "npm publish --registry=https://registry.npmjs.org"
  }
}
