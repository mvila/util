{
  "name": "@mvila/prettierrc",
  "version": "1.1.2",
  "description": "Shared Prettier config used by mvila's projects",
  "keywords": [
    "prettier",
    "shared",
    "config",
    "configuration"
  ],
  "author": "Manuel Vila <hi@mvila.me>",
  "license": "MIT",
